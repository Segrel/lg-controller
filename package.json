{
  "name": "lg-controller",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.5.0",
    "bootswatch": "4.5.0",
    "react": "17.0.1",
    "react-bootstrap": "1.4.3",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
