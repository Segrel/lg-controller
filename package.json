    "sass": "^1.32.5"
  },
  "proxy": "http://localhost:5000",
  "awot-static": {
    "sources": "./build",
    "indexFile": "index.html",
    "sketchDir": "./Server",
    "exclude": [
      "*.map",
      "serviceWorker.js"
    ]
  }
